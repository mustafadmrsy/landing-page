    <authentication mode="Forms">
      <forms loginUrl="~/blogsite/login.aspx" timeout="2880" defaultUrl="~/blogsite/default.aspx" protection="All" />
    </authentication>
    
    <authorization>
      <allow users="*" />
    </authorization>
    
    <customErrors mode="Off" />
    
    <webServices>
      <protocols>
        <add name="HttpGet" />
        <add name="HttpPost" />
        <add name="HttpSoap" />
        <add name="Documentation" />
      </protocols>
      <serviceDescriptionFormatExtensionTypes>
        <add type="System.Web.Script.Services.ScriptServiceExtensionType, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </serviceDescriptionFormatExtensionTypes>
    </webServices>
    
    <httpHandlers>
      <remove path="*.asmx" verb="*"/>
      <add path="*.asmx" verb="*" type="System.Web.Script.Services.ScriptHandlerFactory" validate="false"/>
      <add path="*_AppService.axd" verb="*" type="System.Web.Script.Services.ScriptHandlerFactory" validate="false"/>
      <add path="ScriptResource.axd" verb="GET,HEAD" type="System.Web.Handlers.ScriptResourceHandler" validate="false"/>
    </httpHandlers>
    
    <pages>
      <controls>
        <add tagPrefix="asp" namespace="System.Web.UI" assembly="System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
      </controls>
    </pages>
  </system.web>
  
  <runtime>
